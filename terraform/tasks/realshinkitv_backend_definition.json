[
  {
    "name": "back-container",
    "image": "811331963814.dkr.ecr.ap-northeast-1.amazonaws.com/realshinkitv-back",
    "essential": true,
    "logConfiguration": {
      "logDriver": "awslogs",
      "options": {
        "awslogs-region": "ap-northeast-1",
        "awslogs-stream-prefix": "back",
        "awslogs-group": "/ecs/back"
      }
    },
    "portMappings": [
      {
        "protocol": "tcp",
        "containerPort": 3000,
        "hostPort": 3000
      }
    ],
    "environment": [
      {
        "name": "API_DOMAIN",
        "value": "https://realshinkitv.com"
      },
      {
        "name": "WORKDIR",
        "value": "app"
      },
      {
        "name": "CONTAINER_PORT",
        "value": "3000"
      },
      {
        "name": "RAILS_ENV",
        "value": "production"
      }
    ],
    "secrets": [
      {
        "name": "AWS_ACCESS_KEY_ID",
        "valueFrom": "SHINKI_AWS_ACCESS_KEY_ID"
      },
      {
        "name": "AWS_SECRET_ACCESS_KEY",
        "valueFrom": "SHINKI_AWS_SECRET_ACCESS_KEY"
      },
      {
        "name": "DATABASE_URL",
        "valueFrom": "SHINKI_DATABASE_URL"
      },
      {
        "name": "RAILS_MASTER_KEY",
        "valueFrom": "SHINKI_RAILS_MASTER_KEY"
      }
    ]
  }
]
